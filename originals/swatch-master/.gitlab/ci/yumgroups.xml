<?xml version="1.0" encoding="UTF-8"?>
<comps>
  <group>
    <id>swatch</id>
    <name>swatch</name>
    <default>true</default>
    <description>SWATCH packages of the CACTUS YUM repository</description>
    <uservisible>true</uservisible>
    <grouplist></grouplist>
    <packagelist>
      <packagereq type="mandatory">cactuscore-swatch-logger</packagereq>
      <packagereq type="mandatory">cactuscore-swatch-core</packagereq>
      <packagereq type="mandatory">cactuscore-swatch-action</packagereq>
      <packagereq type="mandatory">cactuscore-swatch-processor</packagereq>
      <packagereq type="mandatory">cactuscore-swatch-system</packagereq>
      <packagereq type="mandatory">cactuscore-swatch-xml</packagereq>
    </packagelist>
  </group>

  <group>
    <id>swatchcell</id>
    <name>swatchcell</name>
    <default>true</default>
    <description>SWATCH TS cell packages of the CACTUS YUM repository</description>
    <uservisible>true</uservisible>
    <grouplist></grouplist>
    <packagelist>
      <packagereq type="mandatory">cactuscore-swatchcellframework</packagereq>
    </packagelist>
  </group>

</comps>
